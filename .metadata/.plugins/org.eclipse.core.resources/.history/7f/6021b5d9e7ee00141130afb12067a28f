package com.wosai.upaydemo.utils;
/**
 * The Util Class for check the String or the widget is empty.(TextView,EditTextView)
 * @author lei
 *
 */
public class CheckUtil {
	
	public static boolean isEmpty(String...strs){
		
		for(String str:strs){
			if(str.isEmpty()){
				return false;
			}
		}
		
	}
 
}
