public class EnvUtil {

	public static DealInfo parseDeal(UpayResult result) {
		DealInfo info = new DealInfo();
		info.setAcount(CheckUtil.isEmpty(result.getAccount()) ? "" : result
				.getAccount());
		info.setAmount(result.getAmount());
	}

